                      onChange={(e) => setPreferences(prev => ({ ...prev, personality: e.target.value }))}
                      className="mt-1"
                    />
                    <div>
                      <div className="font-medium text-gray-900">{type.label}</div>
                      <div className="text-sm text-gray-600">{type.description}</div>
                    </div>
